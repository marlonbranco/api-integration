{
  "name": "api-integration",
  "version": "1.0.0",
  "author": "Marlon Branco",
  "main": "dist/shared/infra/http/server.js",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist/src --copy-files --no-copy-ignored",
    "deploy": "yarn && yarn build && yarn babel src --extensions \".js,.ts\" --out-dir dist/src --copy-files --no-copy-ignored",
    "deploy:docker": "yarn && yarn build && yarn babel src --extensions \".js,.ts\" --out-dir dist/src --copy-files --no-copy-ignored && docker compose up -d",
    "start": "node dist/src/shared/infra/http/server.js",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --inspect --respawn --transpile-only --ignore-watch node_modules src/shared/infra/http/server.ts"
  },
  "dependencies": {
    "axios": "^0.22.0",
    "celebrate": "^15.0.0",
    "class-transformer": "^0.4.0",
    "cors": "^2.8.5",
    "date-fns": "^2.24.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "mongoose": "^5.13.2",
    "node-schedule": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0",
    "xmlbuilder2": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/node": "^7.13.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.12",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.10.2",
    "@types/node-cron": "^3.0.0",
    "@types/node-schedule": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "jest": "^27.2.4",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  }
}
